{
    "$schema": "https://railway.app/railway.schema.json",
    "build": {
        "builder": "DOCKERFILE",
        "buildCommand": "docker build -t lelo-file-data .",
        "dockerfilePath": "file-data/Dockerfile"
    },
    "deploy": {
        "startCommand": "docker run -p 9001:9001 -p 9090:9090 lelo-file-data",
        "restartPolicyType": "ON_FAILURE",
        "restartPolicyMaxRetries": 10
    }
}
