{
  "name": "lelo-web-backend",
  "version": "0.0.1",
  "description": "lelo webstore backend based on medusajs",
  "scripts": {
    "seed": "medusa seed -f ./data/seed.json",
    "migrate": "medusa migrations run",
    "build": "babel src -d dist --extensions \".ts,.js\"",
    "build:admin": "medusa-admin build --deployment",
    "start:admin": "medusa-admin dev",
    "start": "medusa develop"
  },
  "dependencies": {
    "@medusajs/admin": "^5.0.0",
    "@medusajs/cache-redis": "^1.8.6",
    "@medusajs/event-bus-local": "^1.9.3",
    "@medusajs/event-bus-redis": "^1.8.6",
    "@medusajs/medusa": "^1.11.0",
    "@medusajs/medusa-cli": "^1.3.14",
    "medusa-file-minio": "^1.2.0",
    "medusa-fulfillment-manual": "^1.1.37",
    "medusa-interfaces": "^1.3.7",
    "medusa-payment-manual": "^1.0.23",
    "medusa-payment-stripe": "^5.0.0",
    "medusa-plugin-meilisearch": "^2.0.6",
    "typeorm": "^0.3.16"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/preset-typescript": "^7.14.5",
    "babel-preset-medusa-package": "^1.1.19"
  }
}
